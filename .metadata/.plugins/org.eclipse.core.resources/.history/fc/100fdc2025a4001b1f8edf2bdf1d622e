package com.example.cash.vo;

import java.util.Arrays;
import java.util.List;
import java.util.Scanner;
import java.util.stream.Collectors;

public class Test {
	
	static void maxYear(String depts, int max, int current, String dept) {
		Long check = Arrays.stream(depts.replaceAll("[^"+ depts.charAt(current) +"]", "").split("")).collect(Collectors.counting());
		
		
		System.out.println(check);
	}
	
	public static void main(String[] args) {
		Student[] student = new Student[4]; 
		student[0] = new Student("a",1,"1");
		student[1] = new Student("b",2,"2");
		student[2] = new Student("b",3,"3");
		student[3] = new Student("b",4,"1");
		
		maxYear(Arrays.stream(student).map(data->data.getDept()).collect(Collectors.joining()), 0, 0, "0");
	}
}

class Student{
	private String name;
	private int year;
	private String dept;
	
	Student(String name, int year, String dept) {
		this.name = name;
		this.year = year;
		this.dept = dept;
	}
	
	public String getDept() {
		return dept;
	}
}